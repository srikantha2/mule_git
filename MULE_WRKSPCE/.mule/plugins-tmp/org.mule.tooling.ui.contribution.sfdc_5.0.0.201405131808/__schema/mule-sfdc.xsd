          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="url" default="https://login.salesforce.com/services/Soap/u/28.0" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Salesforce endpoint URL</xs:documentation>
          </xs:annotation>
        </xs:attribute>
