package com.bakka.testing.rest.object;

public class Customer {
	
	String id;
	String name;
	String address;
	/**
	 * @return the id
	 */
	public String getId() {
		return id;
	}
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @return the address
	 */
	public String getAddress() {
		return address;
	}
	
	
	public Customer(String id, String name, String address){
		this.id= id;
		this.name= name;
		this.address= address;
	}
	

}
