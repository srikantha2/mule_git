http://www.mulesoft.org/schema/mule/db http://www.mulesoft.org/schema/mule/db/current/mule-db.xsd">
    <jdbc-ee:mysql-data-source name="MySQL_Data_Source" user="root" password="admin" url="jdbc:mysql://localhost:3306/temp" transactionIsolation="UNSPECIFIED" doc:name="MySQL Data Source"/>
    <jdbc-ee:connector name="Database" dataSource-ref="MySQL_Data_Source" validateConnections="true" queryTimeout="-1" pollingFrequency="1000" doc:name="Database">
        <jdbc-ee:query key="contactInfoAll" value="select * from contact;"/>
        <jdbc-ee:query key="familyInfoAlll" value="select e.FirstName, e.LastName, e.emailId, f.FamilyRoleType from employeeinfo e , familyrole f where f.idFamilyRole= e.roleId;"/>
    </jdbc-ee:connector>
    <db:mysql-config name="MySQL_Configuration" host="localhost" port="3306" user="root" password="admin" database="temp" doc:name="MySQL Configuration"/>
    <flow name="sqldbprojectFlow1" doc:name="sqldbprojectFlow1">
        <db:select config-ref="MySQL_Configuration" doc:name="Database" streaming="true">
            <db:parameterized-query><![CDATA[select e.FirstName, e.LastName, e.emailId, f.FamilyRoleType from employeeinfo e , familyrole f where f.idFamilyRole= e.roleId;]]></db:parameterized-query>
        </db:select>
        <logger level="INFO" doc:name="Logger" message="#[payload]"/>
    </flow>
